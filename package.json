  "scripts": {
    "test:server": "mocha server/ --recursive",
    "test:client": "",
    "clean": "rm public/*.bundle.js && rm public/*.map",
    "build:webpack": "NODE_ENV=production webpack --config webpack.config.prod.js",
    "build:webpack:dev": "webpack",
    "build": "npm run clean && npm run build:webpack",
    "build:dev": "npm run build:webpack:dev",
    "build:drywall": "grunt build",
    "build:scss": "cat client/scss/app.scss | node-sass --include-path public/ --include-path client/scss --output-style expanded > public/css/app.css",
    "watch:client": "webpack --progress --colors --watch",
    "watch:server": "NODE_ENV=development nodemon --inspect --watch server index.js",
    "webpack-dev-server": "NODE_ENV=development node server-dev",
    "dev": "parallelshell 'npm run webpack-dev-server' 'npm run watch:server'",
    "generate": "plop --plopfile tools/generators/index.js"
  }
